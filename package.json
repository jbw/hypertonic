{
  "name": "hypertonic",
  "version": "1.0.1",
  "description": "Fitbit API Wrapper",
  "typings": "typings/hypertonic.d.ts",
  "main": "./src/api/index.js",
  "repository": "git+https://github.com/jbw/hypertonic.git",
  "bugs": {
    "url": "https://github.com/jbw/hypertonic/issues"
  },
  "author": "jbw",
  "license": "MIT",
  "dependencies": {
    "append-query": "^2.0.1",
    "axios": "^0.17.1",
    "moment": "^2.20.1"
  },
  "keywords": [
    "fitbit",
    "hypertonic",
    "wrapper"
  ],
  "scripts": {
    "start": "node ./src/api/index.js",
    "watch": "nodemon ./src/api/index.js",
    "test": "mocha",
    "coverage": "nyc --reporter=html --reporter=text-lcov mocha | coveralls",
    "build": "webpack",
    "build:prod": "webpack -p",
    "publish": "npm run build:prod",
    "tslint": "tslint typings/*.ts",
    "lint": "eslint src",
    "typescript": "tsc --project ./typings/tests"
  },
  "nyc": {
    "exclude": [
      "test/**",
      "build"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "exports-loader": "^0.7.0",
    "imports-loader": "^0.7.1",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.7",
    "mocha": "^3.4.2",
    "mocha-nock-fixtures": "^0.2.0",
    "nock": "^9.1.6",
    "nodemon": "^1.11.0",
    "nyc": "^11.4.1",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.15",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0"
  }
}